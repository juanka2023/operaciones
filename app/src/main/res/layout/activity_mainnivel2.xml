<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Mainnivel2">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="23dp"
        android:text="@string/jugador_nombre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="23dp"
        android:text="@string/total_score"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="23dp"
        android:text="@string/et_vidas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <ImageView
        android:id="@+id/ivida"
        android:layout_width="109dp"
        android:layout_height="33dp"
        android:layout_marginStart="210dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="27dp"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tequeda3vida" />

    <ImageView
        android:id="@+id/imageValor2"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="168dp"
        android:layout_marginEnd="3dp"
        android:contentDescription="@string/app_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/siete" />

    <ImageView
        android:id="@+id/imageValor1"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:contentDescription=" "
        app:srcCompat="@drawable/seis"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="173dp" />

    <ImageView
        android:id="@+id/ivsigno"
        android:layout_width="130dp"
        android:layout_height="76dp"
        android:contentDescription="@string/app_name"
        app:srcCompat="@drawable/resta"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="207dp" />

</androidx.constraintlayout.widget.ConstraintLayout>